<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <configuration>
        <wildcard>false</wildcard>
    </configuration>
    <mapping>
        <class-a>accounts.Accounts</class-a>
        <class-b>org.acme.AcmeCustomer</class-b>
        <field>
            <a>orgXyzAccount.company.active</a>
            <b>active</b>
        </field>
        <field>
            <a>orgXyzAccount.company.name</a>
            <b>companyName</b>
        </field>
        <field>
            <a>orgXyzAccount.company.geo</a>
            <b>region</b>
        </field>
        <field>
            <a>orgXyzAccount.contact.city</a>
            <b>city</b>
        </field>
        <field>
            <a>orgXyzAccount.contact.firstName</a>
            <b>firstName</b>
        </field>
        <field>
            <a>orgXyzAccount.contact.lastName</a>
            <b>lastName</b>
        </field>
        <field>
            <a>orgXyzAccount.contact.phone</a>
            <b>phone</b>
        </field>
        <field>
            <a>orgXyzAccount.contact.state</a>
            <b>state</b>
        </field>
        <field>
            <a>orgXyzAccount.contact.streetAddr</a>
            <b>streetAddr</b>
        </field>
        <field>
            <a>orgXyzAccount.contact.zip</a>
            <b>zip</b>
        </field>
    </mapping>
</mappings>
