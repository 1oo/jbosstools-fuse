<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="org.fusesource.ide.launcher.CamelRunMavenLaunchDelegate"
            id="org.fusesource.ide.launcher.camelContext"
            modes="run"
            name="%launcher.camelcontext.name"
            public="true"
            sourceLocatorId="org.eclipse.m2e.launching.MavenSourceLocator"
            sourcePathComputerId="org.eclipse.m2e.launching.MavenSourceComputer">
      </launchConfigurationType>
      <launchConfigurationType
            delegate="org.fusesource.ide.launcher.CamelRunNoTestsMavenLaunchDelegate"
            id="org.fusesource.ide.launcher.camelContextNoTests"
            modes="run"
            name="%launcher.camelcontext.notests.name"
            public="true"
            sourceLocatorId="org.eclipse.m2e.launching.MavenSourceLocator"
            sourcePathComputerId="org.eclipse.m2e.launching.MavenSourceComputer">
      </launchConfigurationType>
   </extension>
   
</plugin>
